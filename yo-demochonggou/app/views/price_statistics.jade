#price_statistics
.header
    .titlea {{bid_name}} ({{bidMessageNO}}人)
    .header-left
        button.btn.btn-primary.btn-lg.btn-block.btn-4(ng-click="return()") 返回

#wrapper.wrapper.scrollable
    ul.list-style-2
        li(ng-repeat="bidgroup in bid_pricegroup track by $index")
            .header-left.count
                .count ￥{{bidgroup.price}}


            .header-right.count-right {{bidgroup.count}}人


footer
    ul.list-style-2
        li.h4.text-center 竞价结果 ：{{bid_success}}


    ul.footer-nav.footer-nav-2tabs
        li
            a(href='#bid_results') 竞价结果
        li.active
            a 价格统计
