#bid_results
.header
    .titlea {{bid_name}} ({{bidMessageNO}}人)
    .header-left
        button.btn.btn-primary.btn-lg.btn-block.btn-4(ng-click="return()") 返回

#bid_resultsModal.modal.fade.center
    .modal-dialog
        .modal-content
            .modal-body
                a.close(data-dismiss="modal" ) &times
                br
                br
                br
                h3.text-center {{bid_successperson}} {{bid_successphone}}XXXXXXXX ￥{{bid_successprice}} 竞价成功！
                br
                br
                br


#wrapper.wrapper.scrollable
    ul.list-style-2
        li(ng-repeat="bidMessage in bidMessages track by $index")
            .header-left.two
                .two {{bidMessage.person_name}}
                .two {{bidMessage.phone_number}}

            .header-right.two-right
                .p.two-right ￥{{bidMessage.bid_price}}


footer
    ul.list-style-2(ng-show="showsuccess")
        li.two.text-center 竞价结果 ：{{bid_successperson}} ￥{{bid_successprice}}    {{bid_successphoneNumber}}

    ul.footer-nav.footer-nav-2tabs
        li.active
            a 竞价结果
        li
            a(href='#price_statistics') 价格统计
